{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "server/index.ts" },
    {
      "source": "/((?!.*\\.(?:css|js|png|jpg|jpeg|svg|webp|ico|txt|xml|map)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/faviconri.png",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0" },
        { "key": "Content-Type", "value": "image/png" }
      ]
    }
  ]
}